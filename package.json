{
  "name": "rolod0x",
  "version": "0.1.0",
  "description": "Your private web3 address book",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aspiers/rolod0x.git"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:firefox": "tsc --noEmit && cross-env __FIREFOX__=true vite build",
    "build:watch": "cross-env __DEV__=true vite build -w --mode development",
    "build:firefox:watch": "cross-env __DEV__=true __FIREFOX__=true vite build -w --mode development",
    "build:hmr": "rollup --config utils/reload/rollup.config.mjs",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "pnpm build:hmr && (run-p wss build:watch)",
    "devtools": "pnpm react-devtools",
    "dev:firefox": "pnpm build:hmr && (run-p wss build:firefox:watch)",
    "test": "jest",
    "test:watch": "jest --watch",
    "commitlint": "commitlint --edit",
    "lint": "run-p -c lint:eslint lint:prettier",
    "lint:watch": "while true; do fd -e ts -e tsx -e js | entr -d pnpm lint; done",
    "lint:fix": "run-p lint:eslint:fix lint:prettier:fix",
    "lint:eslint": "eslint src --max-warnings 0 --ext .ts,.js,.tsx,.jsx",
    "lint:eslint:fix": "pnpm eslint --fix",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "prepare": "husky install"
  },
  "type": "module",
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.1",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@lezer/highlight": "^1.2.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@mui/system": "^5.15.2",
    "@types/webextension-polyfill": "^0.10.7",
    "@uiw/codemirror-themes": "^4.21.21",
    "@uiw/react-codemirror": "^4.21.21",
    "construct-style-sheets-polyfill": "3.1.0",
    "dedent": "^1.5.1",
    "ethers": "^6.9.2",
    "murmurhash": "^2.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.21.3",
    "webext-domain-permission-toggle": "github:fregante/webext-domain-permission-toggle#main",
    "webext-dynamic-content-scripts": "^10.0.0",
    "webext-options-sync": "^4.2.1",
    "webext-permissions": "^3.1.2",
    "webextension-polyfill": "0.10.0"
  },
  "devDependencies": {
    "@codemirror/view": "^6.23.0",
    "@commitlint/cli": "18.2.0",
    "@commitlint/config-conventional": "18.4.3",
    "@rollup/plugin-typescript": "11.1.5",
    "@testing-library/react": "14.0.0",
    "@types/chrome": "0.0.254",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.2",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/ws": "8.5.10",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.17.0",
    "@vitejs/plugin-react": "2.2.0",
    "chokidar": "3.5.3",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "fs-extra": "11.2.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-mock-console": "^2.0.0",
    "lint-staged": "15.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.1.1",
    "react-devtools": "^5.0.0",
    "rollup": "4.9.4",
    "sass": "1.69.5",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "vite": "3.2.7",
    "webpack": "^5.0.0",
    "ws": "8.15.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix --max-warnings 0"
    ]
  },
  "packageManager": "pnpm@8.9.2",
  "peerDependencies": {
    "@codemirror/view": "^6.23.0",
    "webpack": "^5.0.0"
  }
}
